{
  "name": "@souyahia/example",
  "packageManager": "yarn@3.2.4",
  "author": "souyahia",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "prebuild": "yarn run clean",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --check ./src ./test",
    "prettier:fix": "prettier --write ./src ./test",
    "eslint": "eslint --cache --format=pretty ./src ./test",
    "eslint:fix": "eslint --fix --cache --format=pretty ./src ./test",
    "lint": "yarn run prettier && yarn run eslint",
    "lint:fix": "yarn run prettier:fix && yarn run eslint:fix"
  },
  "devDependencies": {
    "@souyahia/eslint-config": "workspace:*",
    "@souyahia/jest-config": "workspace:*",
    "@souyahia/prettier-config": "workspace:*",
    "@souyahia/typescript-config": "workspace:*",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "mkdirp": "^1.0.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "prettier": "@souyahia/prettier-config"
}
